{
    "name": "exaprint/genpdf",
    "description": "PDF Generation tool",
    "keywords": ["pdf"],
    "homepage": "http://www.exaprint.fr",
    "license": "None",
    "minimum-stability": "dev",
    "version": "1.0.1",
    "authors": [
        {
            "name": "Romain Bessuges",
            "email": "romain@exaprint.fr",
            "homepage": "http://www.exaprint.fr/"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "slim/slim": "2.*",
        "slim/views": "0.1.*",
        "twig/twig": "1.*",
        "twig/extensions": "1.0.*@dev",
        "romainbessugesmeusy/php-dsn-factory": "*",
        "romainbessugesmeusy/php-wkhtmltopdf": "*",
        "romainbessugesmeusy/php-sql-query": "*",
        "rbm/tagged-cache": "dev-master",
        "exaprint/php-dal": "*",
        "rbm/php-results-combinator": "*",
        "jdorn/sql-formatter": "*",
        "shuber/curl": "*",
        "myokyawhtun/PDFMerger": "*",
        "menencia/locale-detector": "*",
        "tecnick.com/tcpdf": "dev-master",
        "monolog/monolog": "1.7.*@dev",
        "gettext/gettext": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.3.*@dev"
    },
    "autoload": {
        "psr-0": {
            "Exaprint": "lib/.",
            "Locale": "lib/."
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/exaprint/php-dal"
        },
        {
            "type": "package",
            "package": {
                "name": "myokyawhtun/PDFMerger",
                "version": "1",
                "source": {
                    "url": "https://github.com/myokyawhtun/PDFMerger.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ]
}