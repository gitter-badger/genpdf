{% extends "resources/invoice.twig" %}

{% block type %}
    <div class="type">
        {% if InvoicingType == 3 %}
            Non imponibile art. 8 lett. a) DPR 633/72<br />
            Operazione non soggetta
        {% elseif InvoicingType == 2 %}
            Non imponibile art. 41 DL 331/93<br />
            Inversione contabile
        {% endif %}
    </div>
{% endblock %}

{% block taxcode %}
    <dt>{% trans "Code fiscal" %}</dt>
    <dd>{{ Customer.Siret }}</dd>
{% endblock %}